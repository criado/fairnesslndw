% \setbeamercolor{normal text}{fg=mppetrol!85!black}
\setbeamercolor{normal text}{fg=mpgray}
\setbeamercolor{example text}{parent=normal text}
\setbeamercolor{alerted text}{fg=mppetrol!85!black}

\setbeamercolor{title}{fg=mpgreen,bg=white}
